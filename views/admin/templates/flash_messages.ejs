<% if(error_message.length> 0){ %>
    <script>
        $.toast({
            heading: "<%=error_message %>",
            allowToastClose: false,
            icon: 'error',
            position: 'top-right',
            stack: false,
            loader: false,
            showHideTransition: 'slide'
        })
    </script>
    <% }%>
        <% if(success_message.length> 0){ %>
            <script>
                $.toast({
                    heading: "<%=success_message %>",
                    allowToastClose: false,
                    icon: 'success',
                    position: 'top-right',
                    stack: false,
                    loader: false,
                    showHideTransition: 'slide',
                })
            </script>
            <% }%>